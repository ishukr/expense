/* CREATE TABLE */
CREATE TABLE organisation_expense(
`employeeCode` VARCHAR(100),
`empname` VARCHAR(100),
`jobrole` VARCHAR(100),
`jobTitleName` VARCHAR(100),
`region` VARCHAR(100),
`phoneNumber` DOUBLE,
`emailAddress` VARCHAR(100),
`doj` VARCHAR(100),
`salary` DOUBLE,
`accno` DOUBLE,
`gender` VARCHAR(100)
);

